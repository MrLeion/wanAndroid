<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="QMUILoadingStyle" format="reference"/>

    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference"/>
        <attr name="errorView" format="reference"/>
        <attr name="emptyView" format="reference"/>
        <attr name="noNetworkView" format="reference"/>
        <attr name="contentView" format="reference"/>
    </declare-styleable>

    <declare-styleable name="OptionItemView">
        <attr format="string" name="title"/>
        <attr format="dimension" name="title_size"/>
        <attr format="color" name="title_color"/>
        <attr format="reference|color" name="left_src"/>
        <attr format="string" name="left_text"/>
        <attr format="dimension" name="left_text_size"/>
        <attr format="dimension" name="left_image_margin_left"/>
        <attr format="dimension" name="left_text_margin_left"/>
        <attr format="dimension" name="left_image_margin_right"/>
        <attr format="color" name="left_text_color"/>
        <attr format="reference|color" name="right_src"/>
        <attr format="string" name="right_text"/>
        <attr format="dimension" name="right_text_size"/>
        <attr format="dimension" name="right_image_margin_left"/>
        <attr format="dimension" name="right_image_margin_right"/>
        <attr format="dimension" name="right_text_margin_right"/>
        <attr format="color" name="right_text_color"/>
        <attr format="boolean" name="splite_mode"/>
    </declare-styleable>



    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>


</resources>